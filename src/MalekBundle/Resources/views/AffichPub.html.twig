{% extends "@Front/layout.html.twig" %}


{% block container %}
<!--Start breadcrumb area-->
<section class="breadcrumb-area" style="background-image: url(front/images/resources/breadcrumb-bg.jpg);">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="breadcrumbs">
                    <h1>Buy Ticket</h1>
                </div>
            </div>
        </div>
    </div>
</section>
<!--End breadcrumb area-->




    <!--Start blog area-->
    <section id="blog-area" class="blog-grid-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-md-12 col-sm-12 col-xs-12">
                    <div class="blog-post">
                        <div class="row">
                            <!--Start single blog item-->
                        </div>

    {% for publicite in pub %}
                        <div class="row">
                            <!--Start single blog item-->
                            <div class="col-md-6">
                                <div class="single-blog-item">
                                    <div class="img-holder">
                                        <img src="{{ asset(publicite.imageName) }}" alt="Awesome Image">
                                        <div class="overlay-style-one">
                                            <div class="box">
                                                <div class="content">
                                                    <a href="blog-single.html"><i class="fa fa-link" aria-hidden="true"></i></a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="post-type">
                                            <span class="flaticon-photo"></span>
                                        </div>
                                        <div class="post-date">
                                            <h3> {{publicite.date|date('d') }}<br> {{publicite.date|date('m-Y') }}</h3>
                                        </div>
                                    </div>
                                    <div class="text-holder">
                                        <a href="blog-single.html">
                                            <h3 class="blog-title">{{ publicite.Description }}</h3>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <!--End single blog item-->

                        </div>
    {% endfor %}
                        <!--Start post pagination-->
                        <div class="row">
                            <div class="col-md-12">
                                <ul class="post-pagination text-center">
                                    <li><a href="#"><i class="fa fa-caret-left" aria-hidden="true"></i></a></li>
