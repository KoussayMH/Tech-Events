    <section class="single-speaker-area">
        <div class="container">

            <div class="row">
                <div class="col-md-12">
                    <div class="top-content">
                        <div class="row">
                            <div class="col-md-4">
                                <div class="img-holder">
                                    <img src="{{ asset(ressource.image) }}" alt="Awesome Image">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="text-holder">
                                    <div class="title">
                                        <h2>{{ ressource.prenom }} {{ ressource.nom }} </h2>
                                        <span>{{ ressource.domaine }}</span>
                                    </div>
                                    <div class="text">
                                        <p> {{ ressource.description }}</p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="info-holder">
                                    <ul>
                                        <li><i class="fa fa-building-o" aria-hidden="true"></i> Company: <span>{{ ressource.company }}</span></li>
                                        <li><i class="fa fa-envelope" aria-hidden="true"></i> Email: <span>{{ ressource.email }}</span></li>
                                        <li><i class="fa fa-globe" aria-hidden="true"></i> Website: <span>{{ ressource.website }}</span></li>
                                        <li><i class="fa fa-user" aria-hidden="true"></i> Follow On:
                                            <span>
                                            <a href="#"><i class="fa fa-facebook"></i></a>
                                            <a href="#"><i class="fa fa-twitter"></i></a>
                                            <a href="#"><i class="fa fa-google-plus"></i></a>
                                            <a href="#"><i class="fa fa-pinterest-p"></i></a>
                                            <a href="{{ ressource.link }}"><i class="fa fa-linkedin"></i></a>
                                        </span>
                                        </li>
                                    </ul>
                                </div>
                                <br>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="schedules-button text-center">
                                            <a class="thm-btn" href="{{ path('createDemande', { 'RS': ressource.id }) }}">Demande</a>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>



        </div>
    </section>
